{% include 'BraincraftedBootstrapBundle::_stylesheets.html.twig' %}
<link rel="stylesheet" href="/bundles/braincraftedbootstrapdemo/css/docs.css">
<link rel="stylesheet" href="/bundles/braincraftedbootstrapdemo/css/google-code-prettify.css">

{% include 'BraincraftedBootstrapBundle::_html5shiv.html.twig' %}

</head>

<body data-spy="scroll" data-target=".subnav" data-offset="50">

<div class="navbar navbar-fixed-top">
    <div class="navbar-inner">
        <div class="container">
            <button type="button"class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
                <span class="icon-bar"></span>
            </button>
            <a class="brand" href="{{ path('BraincraftedBootstrapDemoBundle_overview') }}">Bootstrap</a>
            <div class="nav-collapse collapse">
                {{ knp_menu_render('BraincraftedBootstrapDemoBundle:Builder:navBar', { 'nav_type': 'navbar' }) }}
            </div>
        </div>
    </div>
</div>

{% block body %}{% endblock %}

